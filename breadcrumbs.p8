m={
 maze={
  {rm=rms.h,l=2,r=2},        --1
  {rm=rms.h,l=3,r=4},        --2
  {rm=rms.ul,d=5,r=2},       --3
  {rm=rms.h,l=5,r=2},        --4
  {rm=rms.t180,u=3,l=6,r=4}, --5
  {rm=rms.t,l=2,r=7,d=5},			 --6
  {rm=rms.x,u=3,l=6,r=7,d=8},--7
  {rm=rms.v,u=7,d=9},        --8
  {rm=rms.v,u=8,d=10},       --9
  {rm=rms.x,u=10,d=11,l=11,r=6},--10
  {rm=rms.bl,u=10,r=1}
 },
 pos=1,
 dark_mode=false
}

function m:update()
 m.update_position()
 m.update_breadcrumbs()
 m.update_birds()
end

function m:draw()
 map(m.rm.rm[1]*16,m.rm.rm[2]*16,0,0,80,48)
 m.draw_breadcrumbs()
 m.draw_birds()
	if (m.pos==1) then
	 if (m.dark_mode) then
	  print("there is no escape",16,4)
 		print("thanks for playing",16,104)
	 else
 	 print("a game by kent johnson\naka fudgefiddle",16,4)
 		print("made for ludum dare 47\nusing pico-8",16,104)
	 end
 end 
 if m.dark_mode then
	 pal(8,5)
	 pal(12,5)
	 pal(4,7)
	 pal(9,4)
	 pal(5,0)
	 pal(6,8)
	 pal(1,2)
 else
  pal()
 end
end

function m:update_breadcrumbs()
 if not (m.rm.bc == nil) then
	 for i=1, #m.rm.bc do
	  local bc=m.rm.bc[i]
	  if (not (bc==nil)) bc:update()
	 end
	end
end

function m:draw_breadcrumbs()
 if not (m.rm.bc == nil) then
	 for i=1, #m.rm.bc do
	  bc=m.rm.bc[i]
	  if (not (bc==nil)) bc:draw()
	 end
	end
end

function m:draw_birds()
 if not (m.rm.birds==nil) then
  for i=1, #m.rm.birds do
	  b=m.rm.birds[i]
	  if (not (b==nil)) b:draw()
	 end
 end
end

function m:update_birds()
 if not (m.rm.birds == nil) then
  for i=1, #m.rm.birds do
	  b=m.rm.birds[i]
	  if (not (b==nil)) b:update()
	 end
 end
end

function m:update_position()
 -- determine player direction
 -- teleport player 
 rm_x=flr(p.x/128)
 rm_y=flr(p.y/128)
 -- if room change
 if not (rm_x==0) or
    not (rm_y==0) then
  -- reposition to center
  if g.following then
   g.x=60
   g.y=56
  else
   g.x = rand(32,88)
 		g.y = rand(32,88)
  end
  -- delete birds in prev room
  if not (m.rm.birds == nil) then
	  for i=1, #m.rm.birds do
		  b=m.rm.birds[i]
		  if (not (b==nil)) b:destroy()
		 end
	 end
 end
 if (rm_x == -1) then
  m.pos=m.rm.l
  p.x=128
 elseif (rm_x == 1) then
  m.pos=m.rm.r
  p.x=0
 elseif (rm_y == -1) then
  m.pos=m.rm.u
  p.y=128
 elseif (rm_y == 1) then
  m.pos=m.rm.d
  p.y=0
 end
 m.rm=m.maze[m.pos]
end
