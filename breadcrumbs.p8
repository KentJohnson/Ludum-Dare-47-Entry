-- ghost

g={
 active=false,
 appearing=true,
 following=false,
 f_delay=450, -- 15 seconds
 f=6
}

function g:init()
 g.sp=54
 g.delay = rand(150,300) -- 5 - 10 seconds
 g.x = rand(32,88)
 g.y = rand(32,88)
end

function g:update()
 if not m.dark_mode then
	 if (m.pos==5) g.active=true
	 if (m.pos==7) g.appearing=false
	 if g.active then
	  if g.appearing then
		  g.delay-=1
		  if (g.delay<0) then
		   g.update_frame()
		  end
	  elseif g.following then
	   move_to(g,p,0.6)
	  else
	   g.x=60
	   g.y=56
	   g.f_delay-=1
	   if (g.f_delay<0) then
	    g.following=true
	   end
	  end
	 end
	end
end

function g:update_frame()
  g.f-=1
  if (g.f < 0) then
   sfx(sounds.ghost)
   g.sp+=1
   g.f=6
  end
  if (g.sp>56) g.init()
end

function g:draw()
 if not (m.dark_mode) then
	 if g.active then
	  if (g.delay<0) or
	     (g.appearing==false) then
	   spr(g.sp,g.x,g.y)
	  end
	 end
 end
end
