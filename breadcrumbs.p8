-- main

new_game_plus=false
ngp_timer=300
rbt_timer=25

function _init()
 p.init()
 g.init()
end

function _update()
 m.update()
 g.update()
 p.update()
end

function _draw()
 cls()
 if (new_game_plus) then
  print("fatal error",0,0,8)
  rbt_timer-=1
  if (rbt_timer<0) then
   print("rebooting...",0,8,8)
   if (rbt_timer<-25) rbt_timer=50
  end
  ngp_timer-=1
  if (ngp_timer<0) then
   sfx(sounds.siren,-2)
   new_game_plus=false
  end
 else
	 m.draw()
	 g.draw()
	 p.draw()
 end
end

