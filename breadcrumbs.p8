-- utils

function move_to(obj,to,step)
 if (obj.x < to.x) obj.x+=step
 if (obj.x > to.x) obj.x-=step
 if (obj.y < to.y) obj.y+=step
 if (obj.y > to.y) obj.y-=step
end

function in_prox(a,b,d)
	return (a.x > b.x-d)  and
			  (a.x < b.x+(d*2)) and
			  (a.y > b.y-d)  			and
			  (a.y < b.y+(d*2))
end

function rand(a,b)
  if (a>b) a,b=b,a
  return a+flr(rnd(b-a+1))
end
