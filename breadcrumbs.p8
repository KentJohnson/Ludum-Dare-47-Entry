 ul={0,0}, 		-- upper left
 ur={1,0}, 		-- upper right
 bl={0,1}, 		-- bottom left
 br={1,1}, 		-- bottom right
 h={1,2},  		-- horizontal
 v={2,0},  		-- vertical
 t={3,1},    -- t
 t90={3,2},  -- t  90 degrees
 t180={2,2}, -- t 180 degrees
 t270={2,1}, -- t 270 degrees
 x={4,0}     -- plus
