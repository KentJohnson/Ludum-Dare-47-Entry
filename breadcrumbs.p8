-- bird & breadcrumbs

function create_bird(bc)
 local new_bird={
  sp=60,
  y=rand(0,127),
  f_spr=false,
  sound=sounds.bird,
  fly_away=false,
  tte=rand(90,120), -- time to eat
  bc=bc,
  destroy=function(self)
   self.bc.eaten=not self.flying_away
   del(m.rm.birds, self)
  end,
  update=function(self)
   if self.fly_away then
    if self.f_spr then -- fly to right
     self.x+=2
     self.y-=2
    else 											-- fly to left
     self.x-=2
     self.y-=2
    end
    if (self.y<0) then
     del(m.rm.birds, self)
    end
   else
	   -- move to breadcrumb
	   -- if player is near
	   -- fly away
	   move_to(self,self.bc,2)
    if in_prox(self.bc,self,4) then
	    if in_prox(p,self,8) then
	     sfx(self.sound)
		    self.fly_away=true
		    self.bc.decay=rand(120,210)
		   end
		   self.tte-=1
		   if (self.tte<0) then
		    self.bc.eaten=true
		    self.fly_away=true
		   end
		  end
   end
  end,
  draw=function(self)
   spr(self.sp,self.x,self.y,1,1,self.f_spr)
  end
 }
 if (rand(0,1)==0) then -- left
   new_bird.x=0
   new_bird.f_spr=true
 else 																		-- right
  new_bird.x=127
 end
 if (m.dark_mode) then
  new_bird.sp=61
  new_bird.sound=sounds.bird_dark
 end
 if (m.rm.birds == nil) then
  m.rm.birds={new_bird}
 else
  add(m.rm.birds,new_bird)
 end
 sfx(new_bird.sound)
end

function create_breadcrumbs()
 local new_crumb={
  x=p.x,
  y=p.y,
  sp=rand(38,40),
  eaten=false,
  decay=rand(120,210),
  update=function(self)
   self.decay-=1
   if (self.decay==0) then
    create_bird(self)
   end
   if self.eaten then
    del(m.rm.bc,self)
   end
  end,
  draw=function(self)
   spr(self.sp,self.x,self.y)
  end
 }
 if (m.rm.bc==nil) then
  m.rm.bc={new_crumb}
 else
 	add(m.rm.bc, new_crumb)
 end
 sfx(sounds.breadcrumbs)
end
